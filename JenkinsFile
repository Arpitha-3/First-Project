pipeline{
    agent any

    tools {
         maven 'maven'
         jdk 'java'
    }

    stages{
        stage('checkout'){
            steps{
                checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: ' ', url: 'https://github.com/Arpitha-3/First-Project.git']]])
            }
        }
        stage('build'){
            steps{
               bat 'mvn spring-boot:run'
            }
        }
    }
}

